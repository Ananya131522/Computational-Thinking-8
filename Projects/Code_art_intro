# ###############################################
# ### SETUP ###
import turtle
# ###############################################

t = turtle.Turtle()
t.penup()
t.goto(-100, -100)
t.color("purple")
t.pendown()

t.forward(200)
t.left(90)

t.forward(100)
t.left(90)

t.forward(200)
t.left(90)

t.forward(100)
t.left(90)



# ###############################################
# ### ENDING ###
turtle.exitonclick()
# ###############################################
