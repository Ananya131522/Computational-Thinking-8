import turtle  

t = turtle.Turtle()
t.penup()
t.goto(-100, -100)
t.color("cyan")
t.pendown()

for i in range(4):
    t.forward(10)
    t.left(90)

turtle.exitonclick()

import turtle

t = turtle.Turtle()
t.penup()
t.goto( 0, 0)
t.color("cyan")
t.pendown()

for i in range(4):
    t.forward(10)
    t.left(90)

turtle.exitonclick()

